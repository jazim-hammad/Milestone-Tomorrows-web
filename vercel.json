{
  "version": 2,
  "builds": [
    {
      "src": "api/node.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/node.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "regions": ["all"]
}
